URxvt.termName:     rxvt-256color
Xft.dpi:96
Xft.antialias:true
Xft.rgba:rgb
Xft.hinting:true
Xft.hintstyle:hintslight
Xft.autohint:   false
Xft.lcdfilter:  lcddefault
Xcursor.theme: cursor-theme

URxvt.loginShell:true
URxvt.inputMethod:fcitx
URxvt.depth:32

URxvt.mouseWheelScrollPage:true
URxvt.scrollBar:false
URxvt.scrollBar_floating:false
URxvt.scrollstyle:rxvt
URxvt.scrollTtyOutput:false
URxvt.scrollWithBuffer:true
URxvt.scrollTtyKeypress:true

!URxvt.cursorBlink:true
URxvt.cursorBlink:false
URxvt.cursorUnderline:false

URxvt.saveLines:1000
URxvt.borderLess:false

URxvt.allow_bold:false
!URxvt.font:xft:Hack Nerd Font Mono:style=Regular:antialias=True:size=16,xft:Source Han Sans CN:style=Regular:size=20
!URxvt.boldfont:xft:Hack Nerd Font Mono:style=Bold:antialias=True:size=16,xft:Source Han Sans CN:style=Regular:size=20
URxvt.font:xft:Hack Nerd Font Mono:style=Regular:antialias=True:size=16,xft:Source Han Sans SC:style=Regular:size=20
URxvt.boldfont:xft:Hack Nerd Font Mono:style=Bold:antialias=True:size=16,xft:Source Han Sans SC:style=Regular:size=20

URxvt.letterSpace:-1
!URxvt.inheritPixmap:false
!URxvt.transparent:false
URxvt.shading:20
URxvt.intensityStyles: false
!-*- Perl extensions -*-
URxvt.perl-ext-common: default,resize-font,tabbedex,selection-to-clipboard,pasta,matcher,keyboard-select
!URxvt.keysym.M-u:     perl:url-select:select_next
URxvt.url-launcher: /usr/bin/google-chrome-stable
!URxvt.url-launcher:   /usr/bin/firefox
URxvt.underlineURLs:  True
URxvt.matcher.button: 1 
URxvt.keysym.M-Escape:perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.iso14755: false
URxvt.iso14755_52: false
! 一定要写全名及大写的C/V,否则无效
! Shift/Control 顺序无关
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
! 禁用之前的复制粘贴方案
URxvt.keysym.Control-Meta-c: builtin-string:
URxvt.keysym.Control-Meta-v: builtin-string:

!URxvt.clipboard.autocopy: true


!https://github.com/majutsushi/urxvt-font-size
URxvt.keysym.C-Up:     font-size:increase
 URxvt.keysym.C-Down:   font-size:decrease
 URxvt.keysym.C-equal:  font-size:reset

!!######################### Colorscheme settings ########################!


!"颜色透明设置"
!URxvt.inheritPixmap:true
 
rofi.lines: 5
rofi.font: SauceCodePro Nerd Font Mono Light 18
rofi.bw: 3
rofi.loc: 0
rofi.fixed-num-lines: true
rofi.terminal: x-terminal-emulator

! Base16 Eighties
! Scheme: Chris Kempson (http://chriskempson.com)

#define base00 #2d2d2d
#define base01 #393939
#define base02 #515151
#define base03 #747369
#define base04 #a09f93
#define base05 #d3d0c8
#define base06 #e8e6df
#define base07 #f2f0ec
#define base08 #f2777a
#define base09 #f99157
#define base0A #ffcc66
#define base0B #99cc99
#define base0C #66cccc
#define base0D #6699cc
#define base0E #cc99cc
#define base0F #d27b53

rofi.bc: base09
rofi.color-enabled: true
rofi.color-window:  base01, base01, base00
rofi.color-normal:  base01, base05, base01, base02, base07
rofi.color-active:  ase01, base0D, base01, base01, base0D
rofi.color-urgent:  base01, base08, base01, base01, base08
rofi.separator-style: solid


!xTerm.vt100.foreground:white
!xTerm.vt100.background:black
*foreground:   base05
#ifdef background_opacity
*background:   [background_opacity]base00
#else
*background:   base00
#endif
